typedef   struct Vertex{
    double x;
    double y;
    double z;
    Vertex *x_same;
    Vertex *y_same;
    Vertex *next;
}Vertex;
typedef struct {
    double x;
    double y;
    double z;
    Vertex *vertex1;
    Vertex *vertex2;
    Vertex *vertex3;
}Facet;
